{
    "diagnostic.infoSign": "i-",
    "diagnostic.hintSign": -"i",
    "diagnostic.warningSign": -"w",
    "diagnostic.errorSign": -"e",
    "diagnostic.enableSign":true,
    "suggest.enablePreview": true,
    "suggest.triggerAfterInsertEnter": true,
    "suggest.detailMaxLength": 200,
    "suggest.triggerCompletionWait": 1
    "suggest.removeDuplicateItems": true,
    "diagnostic.enableMessage": "always",
    "diagnostic.maxWindowWidth": 800,
    "diagnostic.messageTarget": "echo",
    "diagnostic.virtualTextPrefix": "coc.diag",
    "diagnostic.maxWindowHeight": 400,
    "suggest.maxPreviewWidth": 800,
    "diagnostic.messageDelay": 3,
    "list.maxPreviewHeight": 30,
    "coc.preferences.hoverTarget": "preview",
    "python.autoComplete.extraPaths": 1,
    "coc.preferences.jumpCommand": "jump",
    "npm.binPath": "C:\Program Files (x86)\Yarn\bin"
}
